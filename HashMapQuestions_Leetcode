//	Leetcode 136
	public static int singleNumber(int[] nums) {
		HashMap<Integer, Integer> map = new HashMap<Integer, Integer>();
		for (int num : nums) {
			if (map.containsKey(num))
				map.remove(num);
			else
				map.put(num, 1);
		}
		for (int key : map.keySet()) {
			return key;
		}
		return 0;
	}
